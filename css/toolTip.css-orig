.marker-img-url-holder{
        border: 1px solid #555;
        border-radius: 3px;
        background: rgb(255,255,255);
        width: 100px;
        min-height: 70px;
        height: 100%;
        padding: 2px;
}
.marker-img-url{
        overflow: hidden;
        background-repeat: no-repeat;
        background-size: 90%;
        background-position: center;
        width: 100%;
        height: 100%;
}

.marker-contact-details{
        white-space: nowrap;
        font-style: italic;
}

.marker-contact-details i{
        display: inline-block;
        width: 20px;
}

